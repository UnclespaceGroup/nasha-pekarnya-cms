{
  "kind": "collectionType",
  "connection": "default",
  "collectionName": "products",
  "info": {
    "name": "Продукция"
  },
  "options": {
    "increments": true,
    "timestamps": true
  },
  "attributes": {
    "title": {
      "type": "string"
    },
    "text": {
      "type": "richtext"
    },
    "price": {
      "type": "integer"
    },
    "img": {
      "model": "file",
      "via": "related",
      "plugin": "upload",
      "required": false
    },
    "discount": {
      "type": "decimal"
    },
    "category": {
      "model": "category",
      "via": "products"
    },
    "discountText": {
      "type": "string"
    }
  }
}
